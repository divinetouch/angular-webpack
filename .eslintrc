{
    "rules": {
        "indent": [
            2,
            4
        ],
        "quotes": [
            2,
            "single"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ],
        "no-console": 0,
        "wrap-iife": [2, "outside"],
        "angular/controller-name": [0, "[A/Z].*Controller$/"],
        "angular/typecheck-object": 0,
        "angular/angularelement": 0,
        "angular/timeout-service": 0,
        "angular/log": 0,
        "angular/controller-as": 0,
        "angular/controller-as-route": 0,
        "angular/controller-as-vm" :0
    },
    "plugins": [ 
        "angular"
    ],
    "env": {
        "es6": true,
        "node": true,
        "browser": true,
        "angular": true,
        "jquery": true
    },
    "extends": "eslint:recommended",
    "globals": {
        "angular": true,
        "$": true,
        "google": true
    }
}

